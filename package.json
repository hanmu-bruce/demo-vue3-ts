    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-vue": "^1.9.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^13.0.3",
